author = {Yoo, Chung Yul and Hong, Kun Pyo and Kim, Seung Joo},
doi = {10.1107/S0108270107029563},
file = {:C$\backslash$:/Users/John/Documents/Mendeley Desktop/Acta crystallographica. Section C, Crystal structure communications/2007/Yoo, Hong, Kim/Yoo, Hong, Kim - 2007 - A new layered perovskite, KSrNb2O6F, by powder neutron diffraction.pdf:pdf},
